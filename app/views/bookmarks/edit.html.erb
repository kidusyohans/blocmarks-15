<div class="page-header">
  <h1>Edit Bookmark</h1>
</div>

<%= form_for [@topic, @bookmark] do |f| %>
  <div class="form-group">
    <%= f.label :url %>
    <%= f.text_field :url, class: 'form-control', placeholder: "Enter bookmark url" %>
  </div>
  <%= f.submit "Save", class: 'btn btn-success' %>
<% end %>
